public class dz1 {

    public static void main(String[] args) {
        int x = 0;
        int y = 0;

        int i =0;

        for(int i1 =0; i1 < 1000 ; i1++){
            if(i1 % 5 == 0){
                x += i1;
            }else if(i1 % 3 == 0){
                x += i1;
            }
        }


        i = y + x;



        System.out.println(i);

    }

}
